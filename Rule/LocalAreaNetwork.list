# Domains that should not go through proxy
DOMAIN-SUFFIX,local
DOMAIN-SUFFIX,localhost
DOMAIN-SUFFIX,lan

# Special reserved IPv4 ranges
IP-CIDR,198.18.0.0/16,no-resolve

# Explicit REJECT for invalid/multicast ranges
IP-CIDR,0.0.0.0/8,REJECT,no-resolve
IP-CIDR,224.0.0.0/4,REJECT,no-resolve

# (Optional if you enable IPv6 later)
# IP-CIDR6,::1/128,DIRECT,no-resolve
# IP-CIDR6,fc00::/7,DIRECT,no-resolve
# IP-CIDR6,fe80::/10,DIRECT,no-resolve
# IP-CIDR6,fd00::/8,DIRECT,no-resolve
# IP-CIDR6,ff00::/8,REJECT,no-resolve
